{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=mocklab.db"
  },
  "Mocklab": {
    "UseHostDatabase": false,
    "SchemaName": "mocklab",
    "ConnectionString": "Data Source=mocklab.db",
    "AutoMigrate": true,
    "SeedSampleData": true,
    "EnableUI": true,
    "DatabaseProvider": "sqlite"
  }
}
